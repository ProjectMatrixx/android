  <remote  name="matrixx"
           fetch="https://github.com/ProjectMatrixx"
           sync-c="true"
           sync-j="4"
           revision="refs/heads/14.0" />

  <remote  name="matrixx-devices"
           fetch="https://github.com/Matrixx-Devices"
           sync-c="true"
           sync-j="4"
           revision="refs/heads/14.0" />

